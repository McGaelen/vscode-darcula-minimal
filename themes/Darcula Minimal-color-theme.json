{
	"name": "Darcula Minimal",
	"colors": {
		"editor.background": "#1a1a1a",
		"editor.foreground": "#adadad",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"class": "#5295A4",
		"type": {
			"foreground": "#5295A4",
			"italic": true
		},
		"interface": {
			"foreground": "#5295A4",
			"italic": true
		},
		"variable.defaultLibrary": {
			"foreground": "#9A71A8",
			"bold": true,
			"italic": true
		},
		"parameter": "#9A71A8",
		"variable.local": "#adadad",
		"property": "#adadad"
	},
	"tokenColors": [
		{
			"name": "Keyword, Storage, Operator, Misc",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"operatior",
				"punctuation.separator",
				"meta.brace.square",
				"punctuation.section.embedded",
				"punctuation.definition.keyword",
				"support.type.primitive",
				"entity.name.label",
				"punctuation.terminator.statement",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		// {
		// 	"name": "Parameters, Imported Variables, Global Variables",
		// 	"scope": [
		// 		"variable.parameter",
		// 		"variable.other",
		// 		"punctuation.definition.variable"
		// 	],
		// 	"settings": {
		// 		"foreground": "#9A71A8"
		// 	}
		// },
		{
			"name": "Classes, Tags",
			"scope": [
				"entity.name.tag",
				// "meta.tag.sgml",
				
				// "support.class.component"
			],
			"settings": {
				"foreground": "#5295A4"
			}
		},
		{
			"name": "Literal Values",
			"scope": [
				"constant",
				"string"
			],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"name": "Documentation",
			"scope": [
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#629755"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#585858"
			}
		},
		{
			"name": "XML Tag chevrons and equals",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.separator.key-value.html",
				"punctuation.separator.key-value.svelte",
				"punctuation.definition.template-expression",
				"punctuation.definition.block.begin.svelte",
				"punctuation.definition.block.end.svelte"
			],
			"settings": {
				"foreground": "#666666"
			}
		},
		// {
		// 	"name": "HTML =",
		// 	"scope": [
		// 		"punctuation.separator.key-value"
		// 	],
		// 	"settings": {
		// 		"foreground": "#adadad"
		// 	}
		// }
	]
}
